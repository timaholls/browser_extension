{
  "manifest_version": 3,
  "name": "Auto Proxy Connector",
  "version": "1.0.0",
  "description": "Автоматическое подключение прокси при запуске браузера",
  "permissions": [
    "proxy",
    "storage",
    "management",
    "identity",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background_direct_new.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["config.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}

